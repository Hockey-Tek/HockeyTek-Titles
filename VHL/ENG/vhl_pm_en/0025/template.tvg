		names[19] = template.values('$player-rd32-fname').length > 0;
		names[20] = template.values('$player-rd41-fname').length > 0;
		names[21] = template.values('$player-rd42-fname').length > 0;

		var transitions = [];
		var transitionsHide = [];
		var cnt = 0;
		var cnth = 0;
		var x = 0;
		var y = 0;
		var f = 0;
		var s = 0;

		for(g = 1; g < 23; g++)
		{
			var player = '';
			if(g < 3)
			{
 				player = 'player_g' + g;
			}
			else if(g < 15)
			{
				player = 'player_f' + (g - 2);
				if(s == 0)
				{
					s = 1;
					x = 693;
					y = 0;
				}
			}
			else
			{
				player = 'player_d' + (g - 14);
				if(s == 1)
				{
					s = 2;
					x = -39;
					y = 168;
				}
			}

			if(names[g - 1])
			{
				transitions[cnt++] = {fr : 0, dur : 1, layer : player + '_curtain1', type : 'move', from : '', to :  (349 + x) +','+ (446 + y)};
				transitions[cnt++] = {fr : 0, dur : 1, layer : player + '_curtain2', type : 'move', from : '', to :  (349 + x) +','+  (446 + y)};
