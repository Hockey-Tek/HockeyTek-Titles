				transitions[cnt++] = {fr : 0, dur : 1, layer : player + '_number', type : 'move', from : '', to :  (354 + x) +','+(446 + y)};
				transitions[cnt++] = {fr : 48 + f, layer : player + '_curtain1', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 48 + f, dur : 12, layer :  player + '_curtain1', type : 'ptransform', curve:2, from : '0,36 10,0 10,0 0,36', to : '0,36 10,0 84,0 74,36'},
				transitions[cnt++] = {fr : 60 + f, layer : player + '_number', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 60 + f, layer : player + '_curtain2', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 60 + f, layer : player + '_bkg', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 60 + f, dur : 20, layer :  player + '_curtain1', type : 'ptransform', curve:2, from : '0,36 10,0 84,0 74,36', to : '74,36 84,0 84,0 74,36'},
				transitions[cnt++] = {fr : 65 + f, dur : 20, layer :  player + '_curtain2', type : 'ptransform', curve:2, from : '0,36 10,0 84,0 74,36', to : '74,36 84,0 84,0 74,36'},
				transitions[cnt++] = {fr : 60 + f, layer : player + '_name', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 60 + f, dur : 50, layer :  player + '_name', type : 'clip', curve:2, from : '400,0,400,36', to : '0,0,400,36'},
				transitions[cnt++] = {fr : 60 + f, dur : 60, layer :  player + '_name', type : 'move', curve:2, from : (42 + x) + ',' + (446 + y), to : (442 + x) + ',' + (446 + y)},
				transitions[cnt++] = {fr : 90 + f, layer : player + '_curtain1', type : 'visible', to :  0};
				transitions[cnt++] = {fr : 90 + f, layer : player + '_curtain2', type : 'visible', to : 0};
				transitions[cnt++] = {fr : 80 + f, layer : player + '_line', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 80 + f, dur : 35, layer :  player + '_line', type : 'clip', curve:2, from : '0,0,0,2', to : '0,0,493,2'},
				transitions[cnt++] = {fr : 95 + f, layer : player + '_flag', type : 'visible', to : 1};
				transitions[cnt++] = {fr : 95 + f, dur : 20, layer :  player + '_flag', type : 'opacity', from : 0, to : 1},
				transitions[cnt++] = {fr : 95 + f, dur : 20, layer :  player + '_flag', type : 'move', curve:2, from : (760 + x)+','+(450 + y), to : (861 + x)+','+(450 + y)},

				transitionsHide[cnth++] = {fr : 25, layer : player + '_curtain1', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_curtain2', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_bkg', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_line', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_number', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_flag', type : 'visible', to : 0};
				transitionsHide[cnth++] = {fr : 25, layer : player + '_name', type : 'visible', to : 0};

				x += -12;
				y += 42;
 				f += 2;
			}
		}

		var obj = {name: 'show-main', transition : transitions};
		template.addTransition('show-main', obj);

		obj = {name: 'hide', transition : transitionsHide};
		template.addTransition('hide', obj);
	}
