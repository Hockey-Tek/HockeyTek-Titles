	"script": "
		function upLayerStroke()
		{
			template.orderLayer('stroke_around', '');
		}
		function downLayerStroke()
		{
			template.orderLayer('stroke_around', 'mask_around');
		}
		function checkStatus()
		{
			if(template.values('$match-part') == 2 && template.values('$id-period') < 5)
			{
				command.command ='show-time';
			}
			else
			{
				command.command ='hide-time';
			}
		}
		function showTime()
		{
			match_time.visible = template.values('$id-period') < 5 ? 1 : 0;
		}

		function translate(status)
		{
			var out = status;

			if(status == 'Final')
				out = '<span style=\"color:white; font-size:22px;\">FINAL</span>';

			else if(status == 'Final-SO')
				out = '<span style=\"color:white; font-size:22px;\">FINAL / SO</span>';

			else if(status == 'Final-OT')
				out = '<span style=\"color:white; font-size:22px;\">FINAL / OT</span>';

			else if(status == 'SHOOTOUT')
				out = '<span style=\"color:white; font-size:22px;\">SHOOTOUT</span>';

			else if(status == 'End of 1st')
				out = '<tr style=\"color:white;\"><td style=\"font-size:22px\">END / 1</td><td style=\"font-size:14px; padding-top:3px; padding-left:3px;\">ST</td></tr>';
	
			else if(status == 'End of 2nd')
				out = '<tr style=\"color:white;\"><td style=\"font-size:22px\">END / 2</td><td style=\"font-size:14px; padding-top:3px; padding-left:3px;\">ND</td></tr>';

			else if(status == 'End of 3rd')
				out = '<tr style=\"color:white;\"><td style=\"font-size:22px\">END / 3</td><td style=\"font-size:14px; padding-top:3px; padding-left:3px;\">RD</td></tr>';
						
			else if(status == 'End of OT')
				out = '<span style=\"color:white; font-size:22px;\">END / OT</span>';

			else if(status == 'End of SO')
				out = '<span style=\"color:white; font-size:22px;\">END / SO</span>';

			return out;
		}

		function updateEnd()
		{
			match_status.value = translate(template.values('$match-status'));
			match_status.update = 1;
		}

		function endMatch()
		{
			if(template.values('$match-part') == '3')
			{
				match_status.value = translate(template.values('$match-status'));
				command.command ='end-match';
			}
			else
			{
				command.signal='end-show';
			}
		}

		function resetTransform()
		{
			mask_storke.visible = 0;
			logo_team_r.visible = 0;
			logo_team_l.visible = 0;
			name_team_r.visible = 0;
			name_team_l.visible = 0;
			score_r.visible = 0;
			score_l.visible = 0;
			match_status.visible = 0;
			match_time.visible = 0;
			shadow1_bkg.visible = 0;
			shadow2_bkg.visible = 0;
			top_bkg.visible = 0;
			border.visible = 0;
			black_bkg.visible = 0;
			stroke_around.visible = 0;
			logo_goal.visible = 0;
			text_goal.visible = 0;

			mask_storke.opacity = 1;
			logo_team_r.opacity = 1;
			logo_team_l.opacity = 1;
			name_team_r.opacity = 1;
			name_team_l.opacity = 1;
			score_r.opacity = 1;
			score_l.opacity = 1;
			match_status.opacity = 1;
			match_time.opacity = 1;
			shadow1_bkg.opacity = 1;
			shadow2_bkg.opacity = 1;
			top_bkg.opacity = 1;
			border.opacity = 1;
			black_bkg.opacity = 1;
			stroke_around.opacity = 1;
			logo_goal.opacity = 1;
			text_goal.opacity = 1;
		}
