	"scale" : "1920,1080",
	"rate" : 50.0,
	"values" : {
		"$match-time" : "00:00",
		"$match-period" : "",
		"$id-period" :	"0",
		"$match-status" : "0",
		"$match-part" : "0",
		"$score-left" : "0",
		"$score-right" : "0",
		"$goal-first" : "#ffffff",
		"$goal-second" : "#ffffff",
		"$goal-logo" : "",
