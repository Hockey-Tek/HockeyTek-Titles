			"type" : "image",
			"position" : "95,127",
			"size" : "520,35",
			"path" : "{path}image01.png",
		},
		{
			"name" : "text1",
			"type" : "text",
			"position" : "98,128",
			"size" : "70,30",
			"value" : "$throw-left",
			"font" : "Terminal Dosis,30,50"
			"color" : "255,255,255",
			"scaled" : "1, 0",
			"align" : "0,0"
		},
		{
