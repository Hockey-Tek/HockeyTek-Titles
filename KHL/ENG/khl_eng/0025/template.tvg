				names[6] = template.values('$player-rf22-fname').length > 0;
				names[7] = template.values('$player-rf23-fname').length > 0;
				names[8] = template.values('$player-rf31-fname').length > 0;
				names[9] = template.values('$player-rf32-fname').length > 0;
				names[10] = template.values('$player-rf33-fname').length > 0;
				names[11] = template.values('$player-rf41-fname').length > 0;
				names[12] = template.values('$player-rf42-fname').length > 0;
				names[13] = template.values('$player-rf43-fname').length > 0;
				names[14] = template.values('$player-rd11-fname').length > 0;
				names[15] = template.values('$player-rd12-fname').length > 0;
				names[16] = template.values('$player-rd21-fname').length > 0;
				names[17] = template.values('$player-rd22-fname').length > 0;
				names[18] = template.values('$player-rd31-fname').length > 0;
				names[19] = template.values('$player-rd32-fname').length > 0;
				names[20] = template.values('$player-rd41-fname').length > 0;
				names[21] = template.values('$player-rd42-fname').length > 0;

				var x = 0;
				var y = 0;
				var s = 0;
				var f = 0;
				var cnt = 0;
				var cnth = 0;
				var transitions = [];
				var transitionsHide = [];

				for(g = 1; g < 23; g++)
				{
					var player = '';
					if(g < 3)
					{
 						player = 'g' + g;
					}
					else if(g < 15)
					{
						player = 'f' + (g - 2);
						if(s == 0)
						{
							s = 1;
							x = 706;
							y = 0;
							f = 25;
						}
					}
					else
					{
						player = 'd' + (g - 14);
						if(s == 1)
						{
							s = 2;
							x = 0;
							y = 163;
							f = 4;
						}
					}

					if(names[g - 1])
					{

						transitions[cnt++] = {fr : 0 + f, layer : player + '_player_lineg', type : 'visible', to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 15, layer :  player + '_player_lineg', type : 'move', from : (-352 + x) + ',' + (455 + y), to : (284 + x) + ',' + (455 + y)};

						transitions[cnt++] = {fr : 0 + f, layer : player + '_player_linev', type : 'visible', to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 1, layer :  player + '_player_linev', type : 'move', to : (335 + x) + ',' + (455 + y)};

						transitions[cnt++] = {fr : 0 + f, layer : player + '_player_number', type : 'visible', to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 15, layer :  player + '_player_number', type : 'opacity', from : 0, to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 15, layer :  player + '_player_number', type : 'move', curve:2, from : (-352 + x) + ',' + (456 + y), to : (285 + x) + ',' + (456 + y)};

						transitions[cnt++] = {fr : 0 + f, layer : player + '_player_name', type : 'visible', to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 15, layer :  player + '_player_name', type : 'opacity', from : 0, to : 1};
						transitions[cnt++] = {fr : 0 + f, dur : 15, layer :  player + '_player_name', type : 'move', curve:2, from : (-412 + x) + ',' + (456 + y), to : (345 + x) + ',' + (456 + y)};

						transitions[cnt++] = {fr : 20 + f, layer : player + '_player_flag', type : 'visible', to : 1};
						transitions[cnt++] = {fr : 20 + f, dur : 1, layer :  player + '_player_flag', type : 'move', to : (900 + x) + ',' + (456 + y)};
						transitions[cnt++] = {fr : 20 + f, dur : 15, layer :  player + '_player_flag', type : 'clip', curve:2, from : (0) + ',' + (0) + ',' + (0) + ',' + (40) , to : (0) + ',' + (0) + ',' + (60) + ',' + (40)};
						
						transitionsHide[cnth++] = {fr : 25, layer : player + '_player_number', type : 'visible', to : 0};
						transitionsHide[cnth++] = {fr : 25, layer : player + '_player_name', type : 'visible', to : 0};
						transitionsHide[cnth++] = {fr : 25, layer : player + '_player_flag', type : 'visible', to : 0};
						transitionsHide[cnth++] = {fr : 25, layer : player + '_player_lineg', type : 'visible', to : 0};
						transitionsHide[cnth++] = {fr : 25, layer : player + '_player_linev', type : 'visible', to : 0};

						x += 0;
						y += 40;
 						f += 3;
					}
				}

				var obj = {name: 'show-main', transition : transitions};
				template.addTransition('show-main', obj);

				obj = {name: 'hide', transition : transitionsHide};
				template.addTransition('hide', obj);
				

			}",

