			function initTransition()
			{
				var names = [];

				names[0] = template.values('$player-rg11-fname').length > 0;
				names[1] = template.values('$player-rg12-fname').length > 0;
				names[2] = template.values('$player-rf11-fname').length > 0;
				names[3] = template.values('$player-rf12-fname').length > 0;
				names[4] = template.values('$player-rf13-fname').length > 0;
